{
  "name": "silencenjoyer/rate-limiter-redis",
  "description": "Redis extension for silencenjoyer/rate-limiter",
  "keywords": [
    "silencenjoyer",
    "rate-limiter",
    "rate-limiter-redis"
  ],
  "homepage": "https://github.com/silencenjoyer/redis-counter-redis",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andrew Gebrich",
      "email": "an_gebrich@outlook.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-redis": "*",
    "silencenjoyer/rate-limiter": "^3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Silencenjoyer\\RateLimitRedis\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Silencenjoyer\\RateLimitRedis\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

  },
  "config": {
    "sort-packages": true
  }
}
